{
    "name": "phpunit/phpunit-testlistener-xhprof",
    "description": "A TestListener for PHPUnit that uses XHProf for automated profiling of the tested code",
    "type": "library",
    "keywords": [
        "xunit",
        "xhprof"
    ],
    "homepage": "https://github.com/sebastianbergmann/phpunit-testlistener-xhprof/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Bergmann",
            "email": "sb@sebastian-bergmann.de",
            "role": "lead"
        },
        {
            "name": "Benjamin Eberlei",
            "email": "kontakt@beberlei.de",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/sebastianbergmann/phpunit-testlistener-xhprof/issues",
        "irc": "irc://irc.freenode.net/phpunit"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "ext-xhprof": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*@dev"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "include-path": [
        ""
    ]
}
